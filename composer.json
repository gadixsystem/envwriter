    "keywords": [".env","environment","modify","laravel"],
    "homepage": "https://github.com/gadixsystem/envwriter",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Gadix",
            "email": "gadixinfo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "gadixsystem\\envwriter\\": "src/"
        }
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "squizlabs/php_codesniffer": "^3.6"
    }
